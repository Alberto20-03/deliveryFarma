{
  "version": 2,
  "builds": [
    {
      "src": "server.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    { "src": "/productos", "dest": "/", "methods": ["GET", "OPTIONS"],
    "allow": "https://delivery-farma.vercel.app" },
    { "src": "/crear-usuario", "dest": "/" },
    { "src": "/iniciar-sesion", "dest": "/" },
    { "src": "/historial-pedidos", "dest": "/" },
    { "src": "/dialogo-chat", "dest": "/" },
    { "src": "/pago", "dest": "/" }
  ]
}
